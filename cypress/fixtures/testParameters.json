{
  "info": {
    "title": "Test Parameters",
    "format": {
      "name": {
        "method": ["get", "post", "delete", "etc..."],
        "endpoint": "/controller/endpoint",
        "in": ["query", "body", "path"],
        "expected": ["good", "bad"],
        "parameters": [{"parameterName1": "value1", "parameterName2": "value2"}, { "body": {} }, {"id":  "idValue"}]
      }
    }
  },
  "tests": {
    "minFootprints": {
      "method": "get",
      "endpoint": "/wms/footprints",
      "in": "query",
      "expected": "good",
      "parameters": {
        "layers": "omar:raster_entry",
        "srs": "epsg:4326",
        "bbox": "-180,-90,180,90",
        "width": "1024",
        "height": "512",
        "format": "image/gif"
      }
    },
    "maxFootprints": {
      "method": "get",
      "endpoint": "/wms/footprints",
      "in": "query",
      "expected": "good",
      "parameters": {
        "service": "wms",
        "version": "1.1.1",
        "request": "GetMap",
        "layers": "omar:raster_entry",
        "srs": "epsg:4326",
        "bbox": "-180,-90,180,90",
        "width": "1024",
        "height": "512",
        "format": "image/gif"
      }
    },
    "minWms": {
