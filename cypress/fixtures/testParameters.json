    },
    "minWfs": {
      "method": "get",
      "endpoint": "/wfs",
      "in": "query",
      "expected": "good",
      "parameters": {
        "outputFormat": "JSON",
        "request": "GetFeature",
        "typeName": "omar%3Araster_entry"
      }
    },
    "maxWfs": {
